{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "module": "esnext"
  }
}
